{
  "name": "estarter",
  "version": "1.0.4",
  "description": "",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "type": "module",
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "prepack": "simple-git-hooks",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tf:dev": "nodemon --watch ./transformer/index.ts --exec \"ts-node-esm ./transformer/index.ts\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "simple-git-hooks": {
    "pre-commit": "npx nano-staged",
    "commit-msg": "npx --no -- commitlint --edit ${1}"
  },
  "nano-staged": {
    "connect/*.{js,ts}": "eslint"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
    },
    "extends": [
      "airbnb-base",
      "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest"
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "no-console": "off",
      "import/no-extraneous-dependencies": "off"
    }
  },
  "prettier": {
    "printWidth": 120,
    "bracketSpacing": true,
    "singleQuote": true,
    "endOfLine": "auto"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/generator": "^7.20.7",
    "@babel/helper-plugin-utils": "^7.20.2",
    "@babel/parser": "^7.20.7",
    "@babel/template": "^7.20.7",
    "@babel/traverse": "^7.20.12",
    "@babel/types": "^7.20.7",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@rollup/plugin-commonjs": "^23.0.0",
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "axios": "^1.1.2",
    "cac": "^6.7.14",
    "chalk": "^5.1.0",
    "class-transformer": "^0.5.1",
    "commander": "^9.4.1",
    "connect": "^3.7.0",
    "create-vite": "^3.1.0",
    "download-git-repo": "^3.0.2",
    "eslint": "^8.36.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.2.1",
    "express": "^4.18.2",
    "fs-extra": "^10.1.0",
    "inquirer": "^9.1.3",
    "is-builtin-module": "^3.2.0",
    "kolorist": "^1.7.0",
    "minimist": "^1.2.7",
    "nano-staged": "^0.8.0",
    "nodemon": "^2.0.20",
    "ora": "^6.1.2",
    "prettier": "^2.8.4",
    "prompts": "^2.4.2",
    "puppeteer": "^19.6.2",
    "reflect-metadata": "^0.1.13",
    "rollup": "^2.79.1",
    "rollup-plugin-terser": "^7.0.2",
    "simple-git-hooks": "^2.8.1",
    "ts-morph": "^17.0.1",
    "ts-node": "^10.9.1",
